{
  "name": "brainhole",
  "version": "1.0.0",
  "description": "The backend of Accretion",
  "author": "Fmajor",
  "private": true,
  "scripts": {
    "watch": "cd test; node gen-final-test-watch.js",
    "run": "cross-env NODE_ENV=development nodemon server/index.js --watch server",
    "dev": "cross-env NODE_ENV=development nodemon --watch server --exec 'babel-node --inspect=\"localhost:12345\" server/index.js'",
    "build": "nuxt build",
    "start": "cross-env NODE_ENV=production node server/index.js",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "test": "cd test; node gen-final-test.js; cd ..;ava --fail-fast",
    "test-inspect": "cross-env NODE_ENV=development nodemon --watch test/test-final.js --watch server --exec 'babel-node --inspect node_modules/ava/profile.js test/test-final.js --fail-fast'"
  },
  "nodemonConfig": {
    "ignore": [
      "**/test.js"
    ],
    "delay": "2500"
  },
  "ava": {
    "files": [
      "test/test-final.js"
    ],
    "sources": [
      "./server/**/*.js"
    ],
    "require": [
      "babel-register",
      "babel-polyfill"
    ]
  },
  "dependencies": {
    "@babel/preset-stage-3": "^7.0.0",
    "@nuxtjs/axios": "^5.0.0",
    "axios": "^0.18.0",
    "axios-cookiejar-support": "^0.4.2",
    "body-parser": "^1.18.3",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.5",
    "cross-env": "^5.2.0",
    "deep-equal": "^1.0.1",
    "delay": "^4.1.0",
    "express": "^4.16.3",
    "express-session": "^1.15.6",
    "express-ws": "^4.0.0",
    "install": "^0.12.2",
    "iview": "^3.2.1",
    "lodash": "^4.17.11",
    "mongoose": "^5.4.16",
    "node-yaml": "^3.2.0",
    "nuxt": "^2.0.0",
    "nuxt-sass-resources-loader": "^2.0.5",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^5.0.1",
    "promise": "^8.0.2",
    "sass": "^1.15.3",
    "set-cookie-parser": "^2.2.1",
    "tough-cookie": "^3.0.0"
  },
  "devDependencies": {
    "@babel/polyfill": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/register": "^7.4.0",
    "acorn": "^6.0.5",
    "ava": "^1.4.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.1",
    "babel-loader": "^8.0.5",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2018": "^1.0.0",
    "babel-preset-latest": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "chokidar": "^2.1.2",
    "concat": "^1.0.3",
    "concurrently": "^4.1.0",
    "eslint": "^5.0.1",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-vue": "^4.0.0",
    "node-sass": "^4.12.0",
    "nodemon": "^1.18.9",
    "ps": "^1.0.0",
    "random-int": "^2.0.0",
    "random-words": "^1.1.0",
    "sass-loader": "^7.1.0"
  }
}
