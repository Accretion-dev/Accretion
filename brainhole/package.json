{
  "name": "brainhole",
  "version": "1.0.0",
  "description": "The backend of Accretion",
  "author": "Fmajor",
  "private": true,
  "scripts": {
    "clean": "rm -rf server && mkdir -p server",
    "watch": "npm run clean && bash -c \"$(../dev-scripts/backend/concurrently-command watch)\"",
    "run": "cross-env NODE_ENV=development nodemon server/index.js --watch server",
    "dev": "bash ../dev-scripts/backend/dev-debug",
    "build": "nuxt build",
    "start": "cross-env NODE_ENV=production node server/index.js",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.0.0",
    "body-parser": "^1.18.3",
    "cookie-parser": "^1.4.3",
    "cross-env": "^5.2.0",
    "express": "^4.16.3",
    "express-session": "^1.15.6",
    "express-ws": "^4.0.0",
    "install": "^0.12.2",
    "iview": "^3.2.1",
    "lodash": "^4.17.11",
    "mongoose": "^5.4.2",
    "node-yaml": "^3.1.1",
    "nuxt": "^2.0.0",
    "nuxt-sass-resources-loader": "^2.0.5",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^5.0.1",
    "promise": "^8.0.2",
    "sass": "^1.15.3"
  },
  "devDependencies": {
    "acorn": "^6.0.5",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.1",
    "babel-loader": "^8.0.5",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2018": "^1.0.0",
    "babel-preset-latest": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "concurrently": "^4.1.0",
    "eslint": "^5.0.1",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-vue": "^4.0.0",
    "node-sass": "^4.11.0",
    "nodemon": "^1.18.9",
    "sass-loader": "^7.1.0"
  }
}
